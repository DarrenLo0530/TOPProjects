{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-use-before-define": 0,
        "no-restricted-globals": 0,
        "object-curly-spacing": ["error", "always"],
        "no-underscore-dangle": 0,
        "no-unused-vars": ["error", { "args": "none" }],
        "indent": ["error", 2, { "MemberExpression": 1 }]
    }
}
