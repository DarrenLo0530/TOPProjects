{
  "name": "inventory-backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "babel ./src --out-dir dist --copy-files",
    "start": "npm run dev-start",
    "dev-start": "NODE_ENV=development babel-node ./src/bin/www",
    "dev-server": "nodemon --exec npm run start",
    "prod-start": "NODE_ENV=production babel-node ./src/bin/www",
    "build-css": "postcss src/public/stylesheets/tailwind.css -o src/public/stylesheets/tailwind.css"
  },
  "dependencies": {
    "autoprefixer": "^10.2.5",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "mongoose": "^5.12.5",
    "morgan": "~1.9.1",
    "pug": "^3.0.2",
    "tailwindcss": "^2.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.15",
    "@babel/node": "^7.13.13",
    "@babel/preset-env": "^7.13.15",
    "babel-preset-env": "^1.7.0",
    "eslint": "^7.24.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "nodemon": "^2.0.7",
    "rimraf": "^3.0.2"
  }
}
